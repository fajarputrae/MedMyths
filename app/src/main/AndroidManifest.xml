<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coinbkt.medmyth">

    <application
        android:name=".MedMythApp"
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".FMLibraryActivity" />
        <activity android:name=".FMDetailActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".SoundService"
            android:enabled="true" />

        <activity android:name=".PackActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".AfterGameActivity"></activity>
    </application>

</manifest>